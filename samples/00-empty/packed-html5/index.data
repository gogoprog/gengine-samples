{
    float lutSize = 16.0;
    float scale = (lutSize - 1.0) / lutSize;
    float offset = 1.0 / (2.0 * lutSize);
    #ifndef D3D11
        return tex3D(lut, clamp(color, 0.0, 1.0) * scale + offset).rgb;
    #else
        return lut.Sample(lutSampler, clamp(color, 0.0, 1.0) * scale + offset).rgb;
    #endif
}

